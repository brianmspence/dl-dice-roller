{
  "name": "dl-eval",
  "version": "0.1.0",
  "description": "A dice equation evaluator. Evaluate expressions like '1d20 + 5'.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianmspence/dl-eval.git"
  },
  "keywords": [
    "dice",
    "roll",
    "roller",
    "evaluator"
  ],
  "author": "Brian Spence",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brianmspence/dl-eval/issues"
  },
  "homepage": "https://github.com/brianmspence/dl-eval#readme",
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/mathjs": "^6.0.2",
    "jest": "^24.9.0",
    "prettier": "^2.3.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "mathjs": "^6.2.3"
  },
  "files": [
    "lib/**/*"
  ]
}
